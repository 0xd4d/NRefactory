      <dependency id="Microsoft.CodeAnalysis.Workspaces.Common" version="1.0.0-rc2" />
      <dependency id="Microsoft.Composition" version="1.0.27" />
      <dependency id="System.Collections.Immutable" version="1.1.33-beta" />
      <dependency id="System.Reflection.Metadata" version="1.0.18-beta" />
    </dependencies>
  </metadata>
	<files>
